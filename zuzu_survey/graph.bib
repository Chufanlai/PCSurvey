@INPROCEEDINGS{lee2006task,
  author = {Lee, Bongshin and Plaisant, Catherine and Parr, Cynthia Sims and
	Fekete, Jean-Daniel and Henry, Nathalie},
  title = {Task taxonomy for graph visualization},
  booktitle = {Proceedings of the 2006 AVI workshop on BEyond time and errors: novel
	evaluation methods for information visualization},
  year = {2006},
  pages = {1--5},
  organization = {ACM},
  file = {:/home/lijinglin/Documents/paper/graph/Task Taxonomy for Graph Visualization .pdf:PDF},
  review = {文章指出的说法：
	
	1，群簇是指在空间上聚集在一起的顶点的集合；
	
	2，组内的顶点相互之间具有相同的属性
	
	
	文章对图的任务进行了总结和分类：
	
	1， 连接性。只要用在有向图中，关注顶点之间的连接关系。
	
	2，可通性。有多少点可以到达一个特定的顶点，这些路径中，有哪些路径的长度为n等。
	
	3， 普遍连接性。 对一组的顶点，是否存在这样一些顶点，这些顶点存在到达这组所有顶点的路径。 （这个任务，好像在很多用户使用调研中，都没有考虑过。）
	
	4，连接性，两个顶点之间，是否存在最短路径，图中是否存在群簇，能否找到桥等。
	
	
	5， 对于顶点，用户可以查询具有特定属性的顶点；
	
	6， 对于边，用户可以根据边的权重等属性来筛选边。}
}

